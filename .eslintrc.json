{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off",
        "no-empty-function":"off",
        "no-underscore-dangle":"off",
        "no-alert":"off",
        "no-restricted-globals":"off",
        "import/no-extraneous-dependencies": "off",
        "no-console":"off"
    }
}
